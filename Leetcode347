from collections import Counter


class Solution:
    def topKFrequent(self, nums, k):
        #         if k == len(nums):
        #             return nums
        #         count = Counter(nums)
        #         # print(count.get)

        #         return heapq.nlargest(k, count.keys(), key=count.get)

        res = []
        count = Counter(nums)
        freq = [[] for _ in range(len(nums)+1)]
        for n, c in count.items():
            freq[c].append(n)
        for ct in range(len(freq)-1, 0, -1):
            for num in freq[ct]:

                if k:

                    res.append(num)

                    k -= 1
                else:
                    return res
        return res
