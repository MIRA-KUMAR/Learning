import collections


class Solution:
    def groupAnagrams(self, strs):

        d = collections.defaultdict(list)

        # TN : O(NK)
        for word in strs:
            t = [0]*26
            for ch in word:
                t[ord(ch) - ord('a')] += 1
            d[tuple(t)].append(word)

        return d.values()

        # TN = O(N*KlogK)
        """for word in strs:
            d[tuple(sorted(word))].append(word)
        return d.values()"""
