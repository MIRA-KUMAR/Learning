class Solution:
    def findMaxConsecutiveOnes(self, nums: List[int]) -> int:
        # TN: O(N)
        # SN: O(1)
        c = 0
        count = 0
        for i in range(len(nums)):
            if nums[i] == 0:
                count = 0
            else:
                count += 1
                c = max(c, count)
        return c
